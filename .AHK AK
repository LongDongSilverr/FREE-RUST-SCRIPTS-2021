#NoEn
SendMode Input
 
~F8::Suspend
~End::ExitApp
~F11::Reload
 
LCtrl & ~LButton::
Loop 8

   If GetKeyState("LButton", "LCtrl") {
        Sleep, 6
        mouseXY(-2,3)
    }
    else
    break
Loop 8

   If GetKeyState("LButton", "LCtrl") {
        Sleep, 6
        mouseXY(1,3)
    }
    else
    break
Loop 8

   If GetKeyState("LButton", "LCtrl") {
        Sleep, 6
        mouseXY(-3,3)
    }
    else
    break
Loop 8

   If GetKeyState("LButton", "LCtrl") {
        Sleep, 6
        mouseXY(-3,3)
    }
    else
    break
Loop 8
	If GetKeyState("LButton", "LCtrl") {
